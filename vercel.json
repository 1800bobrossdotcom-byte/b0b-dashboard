{
  "version": 2,
  "name": "b0b-dashboard",
  "builds": [
    {
      "src": "dashboard/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dashboard/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BRAIN_URL": "https://b0b-brain-production.up.railway.app"
  }
}
